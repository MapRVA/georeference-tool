# Django Configuration
DJANGO_SECRET_KEY=your-very-secret-django-key-here
DJANGO_ALLOWED_HOSTS=georeference.example.com
DJANGO_DEBUG=False

# OSM OAuth Configuration
# Get these values by registering an OAuth application at:
# https://www.openstreetmap.org/user/[username]/oauth_clients/new
OSM_URL=https://www.openstreetmap.org
OSM_CLIENT_ID=your-osm-client-id-here
OSM_CLIENT_SECRET=your-osm-client-secret-here

# Secret key for encoding/decoding user data (generate a random string)
OSM_SECRET_KEY=your-random-secret-key-for-encoding

# OAuth callback URL - must match what you configured in OSM
# For production: https://georef.maprva.org/auth/callback/
# For development: http://localhost:8000/auth/callback/
OSM_LOGIN_REDIRECT_URI=https://georeference.maprva.org/auth/callback/
